



CREATE TABLE Hotel( id VARCHAR(16),
					nafn VARCHAR(20),
					rating INT(1),
					description VARCHAR(140),
					zipcode INT(3),
					PRIMARY KEY (id)
				  );

CREATE TABLE Hotel_tag( hotel_id VARCHAR(16),
						tag_name VARCHAR(8),
						FOREIGN KEY (hote_id) REFERENCES Hotel.id
					  );

